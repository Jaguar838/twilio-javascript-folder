var myArgs = process.argv.slice(2);
console.log("myArgs: ", myArgs);

// Initialize a for loop that iterates from 0 myArgs[0]

for (var i = 0; i < myArgs[0]; i++) {
  if (i % 3 == 0 && i % 5 == 0) {
    console.log("ðŸ›° ", i);
    return;
  }
  if (i % 3 == 0) {
    console.log("ðŸ‘½ ", i);
    return;
  }
  if (i % 5 == 0) {
    console.log("ðŸš€ ", i);
    return;
  } else {
    console.log("ðŸ’© ", i);
  }
}
// End of loop