var myArgs = process.argv.slice(2);
console.log('myArgs: ', myArgs);
// Initialize a variable
var i; 
// Initialize a for loop that iterates from 0 myArgs[0]
for (i = 0; i < myArgs[0] i++) { 
if (i%3 == 0) && (  i%5 == 0){console.log(`print ðŸ›° and the ${i} if it is divisible by both 3 and 5`)}
else if (i%3 == 0){console.log(`print ðŸ‘½ and the ${i} if it is divisible by 3`)}
else if (  i%5 == 0){console.log(`print ðŸš€ and the ${i} if it is divisible by 5`)}
else {console.log(`print ðŸ’© and the ${i} if it is not divisible by either 3 or 5`)}
}
